<% html_title l('ai_helper.model_profiles.edit_profile_title')%>
<h2><%= l('ai_helper.model_profiles.edit_profile_title') %></h2>
<%= labelled_form_for @model_profile, url: ai_helper_model_profiles_update_path(id: @model_profile), method: :post, html: {autocomplete: "off"} do |f| %>
 <%= render partial: "form", locals: {f: f} %>

<%= f.submit t(:button_submit) %>
<%= link_to l(:button_cancel), ai_helper_setting_path %>
<% end %>
